---
description: a simple Svelte Component
to: <%- folder %>/<%= Name %>.svelte
files:
    - types
---
<script lang="ts">
<% if !(noTypes) { %>
  import { type <%= Name %> } from "./types.ts";
  type Props = {
    <%= name %>: <%= Name %>
  };
<% } else { %>
  type Props = {
    <%= name %>: string
  };
<% } %>
  const {<%= name %>}: Props = $props();
</script>

<h3><%= Name %></h3>

<% if (condition) { %>
<h1>It worked</h1>
<% } %>
